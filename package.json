{
  "name": "personal-coach",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "echo 'Android development disabled to prevent system crashes. Use web development instead: npm run web'",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "firebase:deploy": "firebase deploy --only firestore:rules,firestore:indexes",
    "firebase:functions": "firebase deploy --only functions",
    "firebase:rules": "firebase deploy --only firestore:rules",
    "firebase:login": "firebase login",
    "debug": "EXPO_PUBLIC_DEBUG_LEVEL=trace expo start --web",
    "debug:ios": "EXPO_PUBLIC_DEBUG_LEVEL=trace expo start --ios",
    "debug:android": "EXPO_PUBLIC_DEBUG_LEVEL=trace expo start --android",
    "debug:inspect": "node --inspect node_modules/.bin/expo start",
    "debug:chrome": "expo start --web --https",
    "debug:clear": "expo start --clear",
    "debug:test": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "debug:profile": "node --cpu-prof --heap-prof node_modules/.bin/expo start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "expo": "~53.0.20",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "firebase": "^12.0.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.12.1",
    "react-native-toast-notifications": "^3.4.0",
    "together-ai": "^0.22.0",
    "victory-native": "^36.9.2",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "tailwindcss": "^3.4.17",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.0",
    "@types/jest": "^29.5.11",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.73.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "firebase-tools": "^13.15.4",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
